{
  "name": "quickstart-node-npm-environment",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "run-p type-check build-only",
    "build-only": "vite build --base=/node-npm-environment/ --outDir target/dist",  
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "cache-clean-force": "npm cache clean --force"
  },
  "dependencies": {
    "pinia": "~2.1.7",
    "vue": "~3.4.25",
    "vue-router": "~4.3.2",
    "axios": "~1.6.8",
    "core-js": "~3.33.3",
    "@popperjs/core": "~2.11.8",
    "bootstrap": "~5.2.3",
    "@vuelidate/core": "~2.0.3",
    "@vuelidate/validators": "~2.0.4",
    "vue-i18n": "~9.11.1",
    "dayjs": "~1.11.10",
    "deepmerge": "~4.3.1"
  },
  "devDependencies": {
    "@babel/types": "~7.23.5",
    "@rushstack/eslint-patch": "~1.6.1",
    "@tsconfig/node20": "~20.1.2",
    "@types/node": "~20.9.0",
    "@vue/eslint-config-prettier": "~8.0.0",
    "@vue/eslint-config-typescript": "~11.0.0",
    "@vue/runtime-dom": "~3.4.25",
    "@vue/tsconfig": "0.4.0",
    "eslint": "~8.53.0",
    "eslint-plugin-vue": "~9.19.2",
    "npm-run-all": "4.1.5",
    "prettier": "~3.1.1",
    "sass": "~1.75.0",
    "sass-loader": "~13.3.2",
    "vue-loader": "~17.4.1",
    "typescript": "~5.3.2",
    "vite": "~5.2.8",
    "vue-tsc": "~2.0.14"
  }
}
